diff --git a/k8s.yml b/k8s.yml
new file mode 100644
index 0000000..bf0e279
--- /dev/null
+++ b/k8s.yml
@@ -0,0 +1,49 @@
+apiVersion: apps/v1
+kind: Deployment
+metadata:
+  name: static-site
+spec:
+  replicas: 1
+  selector:
+    matchLabels:
+      app: static-site
+  template:
+    metadata:
+      labels:
+        app: static-site
+    spec:
+      containers:
+      - name: static-site
+        image: ${OKTETO_BUILD_STATIC_SITE_IMAGE}
+        ports:
+        - containerPort: 80
+---
+apiVersion: v1
+kind: Service
+metadata:
+  name: static-site
+spec:
+  type: ClusterIP
+  ports:
+  - port: 80
+    targetPort: 80
+  selector:
+    app: static-site
+---
+apiVersion: networking.k8s.io/v1
+kind: Ingress
+metadata:
+  name: static-site
+  annotations:
+    dev.okteto.com/generate-host: "true"
+spec:
+  rules:
+  - http:
+      paths:
+      - path: /
+        pathType: Prefix
+        backend:
+          service:
+            name: static-site
+            port:
+              number: 80
\ No newline at end of file
diff --git a/okteto.yml b/okteto.yml
new file mode 100644
index 0000000..9e358a8
--- /dev/null
+++ b/okteto.yml
@@ -0,0 +1,16 @@
+build:
+  static-site:
+    context: .
+    dockerfile: Dockerfile
+
+deploy:
+  - envsubst < k8s.yml | kubectl apply -f -
+
+dev:
+  static-site:
+    image: okteto/static-site:latest
+    command: ["sh"]
+    sync:
+      - .:/usr/share/nginx/html
+    forward:
+      - 8080:80
\ No newline at end of file
